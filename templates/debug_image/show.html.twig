{% extends 'base.html.twig' %}

{% block title %}Image{% endblock %}

{% block body %}
    <h1>Image</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ image.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ image.name }}</td>
        </tr>
        <tr>
            <th>Content</th>
            <td>{{ image.content }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('app_debug_image_index') }}">back to list</a>

    <a href="{{ path('app_debug_image_edit', {'id': image.id}) }}">edit</a>

    {{ include('debug_image/_delete_form.html.twig') }}
{% endblock %}
