{% block title %}Video{% endblock %}

{% block body %}
    <h1>Video</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ video.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ video.name }}</td>
